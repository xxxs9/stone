    <select id="dataCount" parameterType="map" resultType="Integer">
        select count(1) from l_GoodsProduct
        <!--where 1=1
        <if test="productState != null and productState != ''">
            and t.PRODUCT_STATE = #{productState,jdbcType=VARCHAR}
        </if>-->

    </select>


    <delete id="delete" parameterType="java.lang.String">
    delete from l_GoodsProduct
    where ID = #{id,jdbcType=VARCHAR}
  </delete>


    <insert id="insert" parameterType="com.gameloft9.demo.dataaccess.model.system.LenGoodsProduct">
    insert into l_Goodsproduct (
    ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,XINGHAO,
    OTHER1,OTHER2,OTHER3)
    values (
    #{id},
   #{pname},
   #{pprice},
   #{style1},
   #{bianhao},
   #{picihao},
   #{caizhi},
   #{changdi},
   #{yanse},
   #{leixing},
   #{chichun},
   #{zhuangtai},
   #{xinghao},
   #{other1},
   #{other2},
   #{other3}
    )
  </insert>

    <update id="update" parameterType="com.gameloft9.demo.dataaccess.model.system.LenGoodsProduct">
    update l_Goodsproduct
    set
      PNAME=#{pname},
      PPRICE=#{pprice},
      STYLE1=#{style1},
      BIANHAO=#{bianhao},
      PICIHAO=#{picihao},
      CAIZHI=#{caizhi},
      XINGHAO=#{xinghao},
      CHANGDI=#{changdi},
      YANSE=#{yanse},
      LEIXING=#{leixing},
      CHICHUN=#{chichun},
      ZHUANGTAI=#{zhuangtai},
     OTHER1=#{other1},
     OTHER2=#{other2},
     OTHER3=#{other3}
    where ID = #{id,jdbcType=VARCHAR}
  </update>


    <update id="changeState" parameterType="String">
        update l_Goodsproduct set ZHUANGTAI=#{zhuangtai} where ID = #{id,jdbcType=VARCHAR}
    </update>

    <select id="selectByState"  resultMap="BaseResultMap">
        select
         ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,
      XINGHAO,OTHER1,OTHER2,OTHER3
        from l_Goodsproduct where ZHUANGTAI=#{zhuangtai};
    </select>
    <select id="selectByUnProduce"  resultMap="BaseResultMap">
        select
         ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,
      XINGHAO,OTHER1,OTHER2,OTHER3
        from l_Goodsproduct where ZHUANGTAI='未生产';
    </select>






