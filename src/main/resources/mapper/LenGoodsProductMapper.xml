<!--mapper映射-->
<mapper namespace="com.gameloft9.demo.dataaccess.dao.system.LenGoodsProductMapper">
    <!--结果集映射-->
    <resultMap id="BaseResultMap" type="com.gameloft9.demo.dataaccess.model.system.LenGoodsProduct">
        <id column="ID" property="id"/>
        <result column="PNAME" property="pname"/>
        <result column="PPRICE" property="pprice"/>
        <result column="STYLE1" property="style1"/>
        <result column="BIANHAO" property="bianhao"/>
        <result column="PICIHAO" property="picihao"/>
        <result column="CAIZHI" property="caizhi"/>
        <result column="XINGHAO" property="xinghao"/>
        <result column="CHANGDI" property="changdi"/>
        <result column="YANSE" property="yanse"/>
        <result column="LEIXING" property="leixing"/>
        <result column="CHICHUN" property="chichun"/>
        <result column="ZHUANGTAI" property="zhuangtai"/>
            <result column="OTHER1" property="other1"/>
            <result column="OTHER2" property="other2"/>
            <result column="OTHER3" property="other3"/>
    </resultMap>

    <sql id="Base_Column_List">
      ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,
      XINGHAO,OTHER1,OTHER2,OTHER3
     </sql>

    <select id="selectAll" resultMap="BaseResultMap">
        select   ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,
      XINGHAO,OTHER1,OTHER2,OTHER3 from l_GoodsProduct
    </select>
    <select id="getByPrimaryKey" resultMap="BaseResultMap" >
        select
         ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
      YANSE,LEIXING,CHICHUN,ZHUANGTAI,
      XINGHAO,OTHER1,OTHER2,OTHER3
        from l_GoodsProduct
        where ID = #{id,jdbcType=VARCHAR}
    </select>


    <!--mysql版本/分页查询-->
    <select id="selectByPage" resultMap="BaseResultMap">
        select
        ID,PNAME,PPRICE,STYLE1,BIANHAO,PICIHAO,CAIZHI,CHANGDI,
        YANSE,LEIXING,CHICHUN,ZHUANGTAI,
        XINGHAO,OTHER1,OTHER2,OTHER3
         from l_Goodsproduct t
        where 1=1
        <if test="pname != null and pname != ''">
            and t.PNAME = #{pname,jdbcType=VARCHAR}
        </if>
        order by t.id desc
        limit #{start,jdbcType=DECIMAL},#{end,jdbcType=DECIMAL}
    </select>

