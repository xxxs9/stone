<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gameloft9.demo.dataaccess.dao.system.SysMaterialMapper">
    <resultMap id="BaseResultMap" type="com.gameloft9.demo.dataaccess.model.system.SysMaterial">
        <id column="ID" property="id" jdbcType="VARCHAR"/>
        <result column="GOODS_NAME" property="goodsName" jdbcType="VARCHAR"/>
        <result column="GOODS_TYPE" property="goodsType" jdbcType="VARCHAR"/>
        <result column="GOODS_DESCRIBE" property="goodsDescribe" jdbcType="VARCHAR"/>
        <result column="GOODS_SPECIFICATION" property="goodsSpecification" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column_List">
        ID,GOODS_NAME,GOODS_TYPE,GOODS_DESCRIBE,GOODS_SPECIFICATION
    </sql>

    <select id="getGoodsType" resultType="java.lang.String">
        select distinct t.GOODS_TYPE
        from sys_material t
    </select>

    <select id="getGoodsName" resultType="java.lang.String">
        select distinct t.GOODS_NAME
        from sys_material t
    </select>

    <select id="getGoodsSpecification" resultType="java.lang.String">
        select distinct t.GOODS_SPECIFICATION
        from sys_material t
    </select>

    <select id="getAll" resultMap="BaseResultMap" parameterType="map">
        select ID,GOODS_NAME,GOODS_TYPE,GOODS_DESCRIBE,GOODS_SPECIFICATION
        from sys_material t
        where 1=1
        <if test="goodsName != null and goodsName != '' ">
            and (
            t.GOODS_NAME like (concat('%',concat(#{goodsName,jdbcType=VARCHAR},'%')))
            and t.GOODS_NAME >' '
            )
        </if>
        <if test="goodsType != null and goodsType != '' ">
            and t.GOODS_TYPE = #{goodsType,jdbcType=VARCHAR}
        </if>
        <if test="goodsSpecification != null and goodsSpecification != '' ">
            and t.GOODS_SPECIFICATION = #{goodsSpecification,jdbcType=VARCHAR}
        </if>
        limit #{start,jdbcType=DECIMAL},#{end,jdbcType=DECIMAL}
    </select>

    <select id="countGetAll" resultType="Integer" parameterType="map">
        select count(1)
        from sys_material t
        where 1=1
        <if test="goodsName != null and goodsName != '' ">
            and (
            t.GOODS_NAME like (concat('%',concat(#{goodsName,jdbcType=VARCHAR},'%')))
            and t.GOODS_NAME >' '
            )
        </if>
        <if test="goodsType != null and goodsType != '' ">
            and t.GOODS_TYPE = #{goodsType,jdbcType=VARCHAR}
        </if>
        <if test="goodsSpecification != null and goodsSpecification != ''">
            and t.GOODS_SPECIFICATION = #{goodsSpecification,jdbcType=VARCHAR}
        </if>
    </select>

    <insert id="insertSelective" parameterType="com.gameloft9.demo.dataaccess.model.system.SysMaterial">
        insert into sys_material
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                ID,
            </if>
            <if test="goodsName != null and goodsName != '' ">
                GOODS_NAME,
            </if>
            <if test="goodsType != null and goodsType != '' ">
                GOODS_TYPE,
            </if>
            <if test="goodsDescribe != null and goodsDescribe != '' ">
                GOODS_DESCRIBE,
            </if>
            <if test="goodsSpecification != null and goodsSpecification != '' ">
                GOODS_SPECIFICATION,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null and id != '' ">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="goodsName != null and goodsName != '' ">
                #{goodsName,jdbcType=VARCHAR},
            </if>
            <if test="goodsType != null and goodsType != '' ">
                #{goodsType,jdbcType=VARCHAR},
            </if>
            <if test="goodsDescribe != null and goodsDescribe != '' ">
                #{goodsDescribe,jdbcType=VARCHAR},
            </if>
            <if test="goodsSpecification != null and goodsSpecification != '' ">
                #{goodsSpecification,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

    <select id="getByGoodsName" parameterType="java.lang.String" resultMap="BaseResultMap">
        select ID,GOODS_NAME,GOODS_TYPE,GOODS_DESCRIBE,GOODS_SPECIFICATION
        from sys_material t
        where t.GOODS_NAME = #{goodsName,jdbcType=VARCHAR}
    </select>

    <select id="getById" parameterType="map" resultMap="BaseResultMap">
        select ID,GOODS_NAME,GOODS_TYPE,GOODS_DESCRIBE,GOODS_SPECIFICATION
        from sys_material t
        where t.ID = #{id,jdbcType=VARCHAR}
    </select>

    <update id="updateByPrimaryKeySelective" parameterType="com.gameloft9.demo.dataaccess.model.system.SysMaterial" >
        update sys_material
        <set >
            <if test="goodsName != null and goodsName != '' " >
                GOODS_NAME = #{goodsName,jdbcType=VARCHAR},
            </if>
            <if test="goodsType != null and goodsType != '' " >
                GOODS_TYPE = #{goodsType,jdbcType=VARCHAR},
            </if>
            <if test="goodsDescribe != null and goodsDescribe != '' " >
                GOODS_DESCRIBE = #{goodsDescribe,jdbcType=VARCHAR},
            </if>
            <if test="goodsSpecification != null and goodsSpecification != '' " >
                GOODS_SPECIFICATION = #{goodsSpecification,jdbcType=VARCHAR},
            </if>
        </set>
        where ID = #{id,jdbcType=VARCHAR}
    </update>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        delete from sys_material
        where ID = #{id,jdbcType=VARCHAR}
    </delete>
</mapper>