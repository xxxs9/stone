<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gameloft9.demo.dataaccess.dao.system.SysSupplierMapper">
    <resultMap id="BaseResultMap" type="com.gameloft9.demo.dataaccess.model.system.SysSupplier">
        <id column="ID" property="id" jdbcType="VARCHAR"/>
        <result column="SUPPLIER_NAME" property="supplierName" jdbcType="VARCHAR"/>
        <result column="SUPPLIER_DESCRIBE" property="supplierDescribe" jdbcType="VARCHAR"/>
        <result column="CHARGE_NAME" property="chargeName" jdbcType="VARCHAR"/>
        <result column="PHONE" property="phone" jdbcType="VARCHAR"/>
        <result column="EMAIL" property="email" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
       ID,SUPPLIER_NAME,SUPPLIER_DESCRIBE,CHARGE_NAME,PHONE,EMAIL
    </sql>

    <select id="getSupplierName" resultType="java.lang.String">
        select distinct t.SUPPLIER_NAME
        from sys_supplier t
    </select>
    <select id="getAll" resultMap="BaseResultMap" parameterType="map">
        select
        <include refid="Base_Column_List"/>
        from sys_supplier t
        where 1=1
        <if test="supplierName != null and supplierName != '' ">
            and (
            t.SUPPLIER_NAME like (concat('%',concat(#{supplierName,jdbcType=VARCHAR},'%')))
            and t.SUPPLIER_NAME >' '
            )
        </if>
        <if test="start != null and end != null">
            limit #{start,jdbcType=DECIMAL},#{end,jdbcType=DECIMAL}
        </if>
    </select>
    <select id="countGetAll" resultType="Integer" parameterType="map">
        select count(1)
        from sys_supplier t
        where 1=1
        <if test="supplierName != null and supplierName != '' ">
            and (
            t.SUPPLIER_NAME like (concat('%',concat(#{supplierName,jdbcType=VARCHAR},'%')))
            and t.SUPPLIER_NAME >' '
            )
        </if>
    </select>
    <insert id="insertSelective" parameterType="com.gameloft9.demo.dataaccess.model.system.SysSupplier">
        insert into sys_supplier
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null and id != '' ">
                ID,
            </if>
            <if test="supplierName != null and supplierName != '' ">
                SUPPLIER_NAME,
            </if>
                SUPPLIER_DESCRIBE,
            <if test="chargeName != null and chargeName != '' ">
                CHARGE_NAME,
            </if>
            <if test="phone != null and phone != '' ">
                PHONE,
            </if>
            <if test="email != null and email != '' ">
                EMAIL,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null and id != '' ">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="supplierName != null and supplierName != '' ">
                #{supplierName,jdbcType=VARCHAR},
            </if>
                #{supplierDescribe,jdbcType=VARCHAR},
            <if test="chargeName !=null and chargeName != '' ">
                #{chargeName,jdbcType=VARCHAR},
            </if>
            <if test="phone != null and phone != '' ">
                #{phone,jdbcType=VARCHAR},
            </if>
            <if test="email != null and email != '' ">
                #{email,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>
    <select id="getBySupplierName" parameterType="map" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from sys_supplier t
    where t.SUPPLIER_NAME = #{supplierName,jdbcType=VARCHAR}
    </select>
    <select id="getById" parameterType="map" resultMap="BaseResultMap">
      select
      <include refid="Base_Column_List"/>
      from sys_supplier t
      where t.ID = #{id,jdbcType=VARCHAR}
    </select>
    <update id="updateByPrimaryKeySelective" parameterType="com.gameloft9.demo.dataaccess.model.system.SysSupplier" >
        update sys_supplier
        <set>
            <if test="supplierName != null and supplierName != '' " >
                SUPPLIER_NAME = #{supplierName,jdbcType=VARCHAR},
            </if>
                SUPPLIER_DESCRIBE = #{supplierDescribe,jdbcType=VARCHAR},
            <if test="chargeName != null and chargeName != '' ">
                CHARGE_NAME = #{chargeName,jdbcType=VARCHAR},
            </if>
            <if test="phone != null and phone != '' " >
                PHONE = #{phone,jdbcType=VARCHAR},
            </if>
            <if test="email != null and email != '' ">
                EMAIL = #{email,jdbcType=VARCHAR},
            </if>
        </set>
        where ID = #{id,jdbcType=VARCHAR}
    </update>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        delete from sys_supplier
        where ID = #{id,jdbcType=VARCHAR}
    </delete>
</mapper>