    values (
    #{id},
    #{producePlanId},
    #{formulaReachId},
    #{checkUser},
    #{state},
    #{checkTime},
    #{wasteId},
    #{checkRemark},
    #{other1},
    #{other2},
    #{other3}
    )
  </insert>
