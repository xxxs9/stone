    <update id="update" parameterType="com.gameloft9.demo.dataaccess.model.system.LenProduct">
    update l_product_check
    set
    ID=#{id},
    PRODUCE_PLAN_ID=#{producePlanId},
    FORMULA_REACH_ID=#{formulaReachId},
    CHECK_USER=#{checkUser},
    STATE=#{state},
    CHECK_TIME#{checkTime},
    WASTE_ID= #{wasteId},
    CHECK_REMARK=#{checkRemark},
    OTHER1=#{other1},
    OTHER2=#{other2},
    OTHER3=#{other3}

  </update>


    <update id="changeState" parameterType="String">
        update l_product_check set STATE=#{state} where ID = #{id,jdbcType=VARCHAR}
