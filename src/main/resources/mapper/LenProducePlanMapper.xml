    <update id="update" parameterType="com.gameloft9.demo.dataaccess.model.system.LenProduct">
    update l_produce_plan
    set

     PRODUCT_ID=#{productId},
     PLAN_NUMBER=#{planNumber},
     REAL_NUMBER=#{realNumber},
     STATE=#{state},
     GOODS_NUMBER=#{goodNumber},
     PRODUCE_DATE=#{produceDate},
     FINISH_DATE= #{finishDate},
     BILL_DATE=#{billDate},
     BILL_CYCLE=#{billCycle},
     PLAN_REMARK=#{planRemark},
     OTHER1=#{other1},
     OTHER2=#{other2},
     OTHER3=#{other3}
    where ID = #{id,jdbcType=VARCHAR}
  </update>


    <update id="changeState" parameterType="String">
        update l_produce_plan set STATE=#{state} where ID = #{id,jdbcType=VARCHAR}
    </update>
    <update id="changeOther1" parameterType="String">
