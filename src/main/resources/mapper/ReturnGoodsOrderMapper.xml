    <select id="findAll" resultMap="ShipmentOrderResultMap">

        select
        *
        from
        w_shipment_order  left join w_return_goods_order ON w_shipment_order.ID=w_return_goods_order.ID

        <where>
            <if test="goodsName != null and goodsName != ''">
                and GOODS_NAME = #{goodsName,jdbcType=DATE}
            </if>
            w_shipment_order.STATE != "收货成功" and w_shipment_order.STATE != "等待收货" and w_shipment_order.STATE != "等待发货"
        </where>

        limit #{start,jdbcType=DECIMAL},#{end,jdbcType=DECIMAL}
    </select>



    <!--查找条数-->
    <select id="countGetAll" resultType="int" parameterType="map">

        select
        count(1)
        from w_return_goods_order t
        <where>
            <if test="goodsName != null and goodsName != ''">
                and t.GOODS_NAME = #{goodsName,jdbcType=VARCHAR}
            </if>

        </where>
    </select>


    <!--通过ID删除-->
    <delete id="deleteById" parameterType="java.lang.String">
    delete from w_return_goods_order
    where ID = #{id}
  </delete>
    <!--添加-->
    <insert id="add" parameterType="com.gameloft9.demo.dataaccess.model.system.ReturnGoodsOrder">
        insert into
           w_return_goods_order
          ( ID,
        GOODS_ID,
        GOODS_NAME,
        CUSTOMER,
        GOODS_NUMBER,
        GOODS_AMOUNT,
        APPLY_USER,
        APPLY_TIME,
        STATE,
        AUDIT_USER,
        AUDIT_TYPE,
        REMARKS)
        values
          (#{id},
          #{goodsId},
          #{goodsName},
          #{customer},
          #{goodsNumber},
          #{goodsAmount},
          #{applyUser},
          #{applyTime},
          #{state},
          #{auditUser},
          #{auditType},
          #{remarks})
    </insert>
    <!--修改-->
    <update id="update" parameterType="com.gameloft9.demo.dataaccess.model.system.ShipmentOrder">
        update
            w_shipment_order
        set
            GOODS_ID=#{goodsId},
           GOODS_NAME=#{goodsName},
            CUSTOMER=#{customer},
            GOODS_NUMBER=#{goodsNumber},
            GOODS_AMOUNT=#{goodsAmount},
            APPLY_USER=#{applyUser},
            APPLY_TIME=#{applyTime},
            STATE=#{state},
            AUDIT_USER=#{auditUser},
            AUDIT_TYPE=#{auditType},
            REMARKS=#{remarks}
        where
            ID=#{id}
  </update>




