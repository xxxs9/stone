    </update>

    <!--获取产品id下拉框-->
   <!-- <select id="getProductId" parameterType="java.lang.String">
        select DISTINCT
        PRODUCT_ID
        from
          w_marker_order

    </select>-->

    <!--获取角色ID-->

    <select id="getById" resultMap="ReturnGoodsOrderResultMap" parameterType="java.lang.String">
        select
            ID,
        GOODS_ID,
        GOODS_NAME,
        CUSTOMER,
        GOODS_NUMBER,
        GOODS_AMOUNT,
        APPLY_USER,
        APPLY_TIME,
        STATE,
        AUDIT_USER,
        AUDIT_TYPE,
        REMARKS
        from
          w_return_goods_order
        where
          ID = #{id,jdbcType=VARCHAR}
    </select>

    <!--查找所有分页模糊查询-->
  <!--  <select id="findAll"  parameterType="map" resultMap="ReturnGoodsOrderResultMap">
        select
        <include refid="Base_Column_List"/>
