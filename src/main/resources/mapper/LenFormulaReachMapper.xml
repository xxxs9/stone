    <sql id="Base_Column_List">
      ID,PRODUCT_ID,PRODUCE_FORMULA_ID,PRODUCE_FORMULA_DETAIL_ID,FORMULA_BACK,DEPOT_AUDI,STATE,REACH_USER,REACH_TIME,OTHER1,OTHER2,OTHER3
     </sql>

    <select id="selectAll" resultMap="BaseResultMap">

        select  ID,PRODUCT_ID,PRODUCE_FORMULA_ID,FORMULA_BACK,PRODUCE_FORMULA_DETAIL_ID,DEPOT_AUDI,STATE,REACH_USER,REACH_TIME,OTHER1,OTHER2,OTHER3 from l_formula_reach
    </select>
    <select id="getByPrimaryKey" resultMap="BaseResultMap" >
        select
        ID,PRODUCT_ID,PRODUCE_FORMULA_ID,FORMULA_BACK,PRODUCE_FORMULA_DETAIL_ID,DEPOT_AUDI,STATE,REACH_USER,REACH_TIME,OTHER1,OTHER2,OTHER3
        from l_formula_reach
        where ID = #{id,jdbcType=VARCHAR}
    </select>


    <!--mysql版本/分页查询-->
    <select id="selectByPage" resultMap="BaseResultMap">
        select ID,PRODUCT_ID,PRODUCE_FORMULA_ID,FORMULA_BACK,PRODUCE_FORMULA_DETAIL_ID,DEPOT_AUDI,STATE,REACH_USER,REACH_TIME,OTHER1,OTHER2,OTHER3 from l_formula_reach t
        where 1=1
        <if test="productId != null and productId != ''">
            and t.PRODUCT_ID= #{productId,jdbcType=VARCHAR}
        </if>
        <if test="reachUser != null and reachUser != ''">
            and t.REACH_USER = #{reachUser,jdbcType=VARCHAR}
        </if>
        order by t.id desc
        limit #{start,jdbcType=DECIMAL},#{end,jdbcType=DECIMAL}
    </select>

