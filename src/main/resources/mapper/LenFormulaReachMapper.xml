    <update id="update" parameterType="com.gameloft9.demo.dataaccess.model.system.LenFormulaReach">
    update l_formula_reach
    set

     PRODUCT_ID=#{productId},
     PRODUCE_FORMULA_ID=#{produceFormulaId},
     PRODUCE_FORMULA_DETAIL_ID= #{produceFormulaDetailId},
     STATE= #{state},
     DEPOT_AUDI=#{depotAudi},
     REACH_USER= #{reachUser},
     REACH_TIME=#{reachTime},
     FORMULA_BACK=#{formulaBack},
     OTHER1=#{other1},
     OTHER2=#{other2},
     OTHER3=#{other3}
    where ID = #{id,jdbcType=VARCHAR}
  </update>


    <update id="changeState" parameterType="String">
        update l_formula_reach set STATE=#{state} where ID = #{id,jdbcType=VARCHAR}
    </update>

    <update id="changeDepot" parameterType="String">
        update l_formula_reach set DEPOT_AUDI=#{depot} where ID = #{id,jdbcType=VARCHAR}
    </update>
    <select id="getByProductId" resultMap="BaseResultMap">
        select * from stone.l_formula_reach fr where fr.PRODUCT_ID=#{productId}
    </select>
